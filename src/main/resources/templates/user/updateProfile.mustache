    <main class="main-size">
        <div class="card">
            <div class="card-header"><b>회원 정보 수정</b></div>
            <div class="card-body">
                <form action="/user/update" method="get" enctype="application/x-www-form-urlencoded"
                    onsubmit="return true">
                    <div class="d-flex justify-content-end"><b>기본 정보 수정</b></div>
                    <div>아이디</div>
                    <div class="mb-3">
                        <input type="text" class="form-control" placeholder="Enter username..." name="username">
                    </div>
                    <div>비밀번호</div>
                    <div class="mb-3">
                        <input type="password" class="form-control" placeholder="Enter password..." name="password">
                    </div>
                    <div>이메일</div>
                    <div class="mb-3">
                        <input type="email" class="form-control" placeholder="Enter email..." name="email">
                    </div>
                    <div class="d-flex justify-content-end"><b>개인 정보 수정</b></div>
                    <div>이름</div>
                    <div class="mb-3">
                        <input type="text" class="form-control" value="홍길동" name="address" disabled>
                    </div>
                    <div>생년월일</div>
                    <div class="mb-3">
                        <input type="date" class="form-control" name="birthyear" placeholder="1" required>
                    </div>
                    <div>성별</div>
                    <div class="mb-3">
                        <div class="form">
                            <span>남자</span>
                            <input type="radio" name="gender" value="male">
                            <span>여자</span>
                            <input type="radio" name="gender" value="female">
                        </div>
                    </div>
                    <div>휴대전화</div>
                    <div class="mb-3">
                        <input type="tel" class="form-control" placeholder="010-0000-0000" name="number" required>
                    </div>
                    <div>주소</div>
                    <div class="mb-3">
                        <input type="text" class="form-control" placeholder="부산진구 ..." name="address">
                    </div>
                    <div>보유 기술</div>
                    <div class="mb-3">
                        <span>java</span>
                        <input type="checkbox" name="stack" value="java">
                        <span>javaScript</span>
                        <input type="checkbox" name="stack" value="javascript">
                        <span>Spring</span>
                        <input type="checkbox" name="stack" value="spring">
                        <span>HTML</span>
                        <input type="checkbox" name="stack" value="html">
                        <span>jQuery</span>
                        <input type="checkbox" name="stack" value="jQuery">
                        <span>MySQL</span>
                        <input type="checkbox" name="stack" value="mysql">
                    </div>

                    <button type="submit" class="btn btn-primary form-control">수정 완료</button>
                </form>
            </div>
        </div>
        <!-- 바디 end -->
    </main>
